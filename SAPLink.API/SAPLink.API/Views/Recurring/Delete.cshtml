@model int

@{
    Layout = null;
}


<h2>Delete Recurrence</h2>

<p>Are you sure you want to delete this recurrence?</p>

@using (Html.BeginForm("Delete", "Recurring", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    <input type="hidden" name="id" value="@Model" />

    <input type="submit" value="Delete" class="btn btn-default" />
}